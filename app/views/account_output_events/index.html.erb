<h1>Listing account_output_events</h1>

<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>Account</th>
    <th>Value</th>
  </tr>

<% @account_output_events.each do |account_output_event| %>
  <tr>
    <td><%= account_output_event.account.name %></td>
    <td><%= account_output_event.value %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Account output event', new_account_output_event_path %>
